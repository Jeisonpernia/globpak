<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- QWeb Reports -->
        <report 
            id="account_payable_voucher"
            model="account.invoice"
            string="Accounts Payable Voucher"
            report_type="qweb-pdf"
            name="globpak.report_account_payable_voucher"
            file="globpak.report_account_payable_voucher"
            attachment_use="True"
            attachment="(object.state in ('open','paid')) and ('APV'+(object.number or '').replace('/','')+'.pdf')"
        />

        <!--<report 
            id="check_voucher"
            model="account.payment"
            string="Print Check Voucher"
            report_type="qweb-pdf"
            name="globpak.report_check_voucher"
            file="globpak.report_check_voucher"
            attachment_use="True"
            attachment="(object.state in ('posted','sent')) and ('CHK'+(object.name or '').replace('/','')+'.pdf')"
        />-->

    </data>
</odoo>
